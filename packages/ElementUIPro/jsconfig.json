{
	"compilerOptions": {
		"jsx": "preserve",
		"target": "es5",
		"module": "esnext",
		"baseUrl": "./",
		"moduleResolution": "node",
		"paths": {
			"@/*": [
				"src/*"
			]
		},
		"lib": [
			"esnext",
			"dom",
			"dom.iterable",
			"scripthost"
		]
	},
	"vueCompilerOptions": {
		"target": 2
	},
	"exclude": [
		"node_modules",
		"dist",
		"public",
		"static"
	],
}
